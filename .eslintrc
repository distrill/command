{
    "extends": "airbnb/base",
    "rules": {
        "no-multiple-empty-lines": [2, {"max": 3, "maxEOF": 1}],
        "no-console": 0,
        "no-param-reassign": [2, { "props": false }],
        "arrow-body-style": 0,
        "prefer-template": 0,
        "padded-blocks": 0,
        "max-len": 0,
        "object-shorthand": 0,
        "no-use-before-define": 0
    },
    "env": {
      "node": true,
      "mocha": true
    }
}